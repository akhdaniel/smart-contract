        <Layout display="display" className="'o_dashboard h-100'">

            <!-- filter-->
            <div class="row px-3 py-2">
                <div class="col-12 col-md-3">
                    <h1>
                        <t t-if="state.selectedKanwil">
                            Dashboard <t t-esc="state.selectedKanwil.name"/>
                        </t>
                        <t t-else="">
                            Dashboard
                        </t>
                    </h1>
                    <h3>
                        <t t-if="state.selectedMitra">
                            Mitra: <t t-esc="state.selectedMitra.name"/>
                        </t>
                    </h3>
                </div>






                <div class="col-12 col-md-3">
                    <div class="form-control px-1 m-0">
                        <select
                            t-on-change="onKanwilChange"
                            class="form-select border-0"
                        >

                            <option value="">-- Pilih Kanwil --</option>
                            <t t-foreach="state.kanwils" t-as="kw" t-key="kw.id">
                                <option t-att-value="kw.id" t-esc="kw.name"/>
                            </t>


                        </select>
                    </div>
                </div>


                <div class="col-12 col-md-2">
                    <div class="form-control px-1 m-0">
                        <select
                            t-on-change="onYearChange"
                            class="form-select border-0"
                        >
                            <option value="">-- Pilih Tahun --</option>
                            <t t-foreach="state.availableYears" t-as="th" t-key="th">
                                <option 
                                    t-att-value="th"
                                    t-att-selected="state.selectedYear &amp;&amp; state.selectedYear.id == th ? true : undefined"
                                    t-esc="th"/>
                            </t>
                        </select>
                    </div>
                </div>





